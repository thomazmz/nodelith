{
  "task.allowAutomaticTasks": "on",
  "search.exclude": {
    "**/node_modules": true,
    "**/.volumes": true,
    "**/coverage": true,
    "**/dist": true,
    "**/.git": true,
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/.volumes": true,
    "**/cache": true,
    "**/dist": true,
  },
  "files.associations": {
    "*.js": "javascript",
    "*.ts": "typescript",
    "*.json": "json"
  }
}