{
  "name": "@nodelith/example",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "exports": {
    "./*": {
      "types": "./dist/*/*.library.d.ts",
      "import": "./dist/*/*.library.js",
      "require": "./dist/*/*.library.js"
    }
  },
  "scripts": {
    "build": "tsc --project ./build.json",
    "start": "node --inspect --enable-source-maps --env-file=.env dist/application/server.js"
  },
  "dependencies": {
    "@nodelith/controller": "workspace:^",
    "@nodelith/injection": "workspace:^",
    "@nodelith/express": "workspace:^",
    "@nodelith/config": "workspace:^",
    "@nodelith/core": "workspace:^",
    "@nodelith/http": "workspace:^",
    "express": "^5.0.3",
    "acorn": "^8.15.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2",
    "tsx": "^4.19.0"
  }
}
